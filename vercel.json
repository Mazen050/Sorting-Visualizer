{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
